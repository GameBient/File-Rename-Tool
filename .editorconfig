root = true

[*]
charset = utf-8
indent_size = 2
trim_trailing_whitespace = true
end_of_line = crlf

[*.cs]
indent_size = 4
indent_style = space
insert_final_newline = true
max_line_length = 100

[.editorconfig]
insert_final_newline = true

[*.xml]
indent_size = 4
indent_style = space
insert_final_newline = true
